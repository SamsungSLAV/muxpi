static uint8_t oled_buffer[(128)][(32) / 8];
static int oled_pos_x = 0;
static int oled_pos_y = 0;

void oled_init(void) {
    rcc_periph_clock_enable(RCC_SPI1);

    gpio_mode_setup((((0x40000000U) + 0x08000000) + 0x0000), 0x1, 0x0, (1 << 3) | (1 << 6) | (1 << 4));

    gpio_mode_setup((((0x40000000U) + 0x08000000) + 0x0000), 0x2, 0x0, (1 << 5) | (1 << 7));
    gpio_set_af((((0x40000000U) + 0x08000000) + 0x0000), 0x0, (1 << 5) | (1 << 7));

    spi_set_baudrate_prescaler((((0x40000000U) + 0x00000000) + 0x13000), 0x3);
    spi_set_clock_polarity_1((((0x40000000U) + 0x00000000) + 0x13000));
    spi_set_clock_phase_1((((0x40000000U) + 0x00000000) + 0x13000));
    spi_set_master_mode((((0x40000000U) + 0x00000000) + 0x13000));

    spi_set_data_size((((0x40000000U) + 0x00000000) + 0x13000), (0x7 << 8));
    spi_enable_ss_output((((0x40000000U) + 0x00000000) + 0x13000));

    spi_fifo_reception_threshold_8bit((((0x40000000U) + 0x00000000) + 0x13000));
    spi_set_nss_high((((0x40000000U) + 0x00000000) + 0x13000));
    spi_enable((((0x40000000U) + 0x00000000) + 0x13000));
}

static void oled_write_data(const unsigned char dat) {
    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));
    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 3));
    gpio_clear((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));

    spi_send8((((0x40000000U) + 0x00000000) + 0x13000), dat);
    while (((*(volatile uint32_t *)(((((0x40000000U) + 0x00000000) + 0x13000)) + 0x08)) & (1 << 7)));

    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));

    oled_buffer[oled_pos_x][oled_pos_y / 8] = dat;
}

static void oled_write_cmd(unsigned char cmd) {
    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));
    gpio_clear((((0x40000000U) + 0x08000000) + 0x0000), (1 << 3));
    gpio_clear((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));

    spi_send8((((0x40000000U) + 0x00000000) + 0x13000), cmd);
    while (((*(volatile uint32_t *)(((((0x40000000U) + 0x00000000) + 0x13000)) + 0x08)) & (1 << 7)));

    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 4));
}

static void oled_set_pos_for_text(int x, int y) {
    y = y/8 + y%8;
    oled_write_cmd(0xb0 + y);
    oled_write_cmd(((x & 0xf0) >> 4) | 0x10);
    oled_write_cmd((x & 0x0f));
}

static void oled_set_pos_for_point(int x, int y) {
    oled_pos_x = x;
    oled_pos_y = y;

    y = y/8;
    oled_write_cmd(0xb0 + y);
    oled_write_cmd(((x & 0xf0) >> 4) | 0x10);
    oled_write_cmd((x & 0x0f));
}

static void oled_reset(void) {
    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 6));
    for (volatile int d = 0; d < 99; d++);
    gpio_clear((((0x40000000U) + 0x08000000) + 0x0000), (1 << 6));
    for (volatile int d = 0; d < 99; d++);
    gpio_set((((0x40000000U) + 0x08000000) + 0x0000), (1 << 6));
}

